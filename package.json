{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "En enkel Go Web Applikasjon som bygger, serverer og verfiserer JWTs basert p√• email/password hentet fra en PostgreSQL database",
  "scripts": {
    "start": "./auth-service",
    "dev": "concurrently \"npm run dev:service\" \"npm run dev:ui\"",
    "dev:service": "go run .",
    "dev:ui": "vite ui",
    "build": "npm run build:service && npm run build:ui",
    "build:service": "go build .",
    "build:ui": "tsc -b ui && vite build ui/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olehalv/auth-service.git"
  },
